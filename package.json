{
  "name": "go-genre",
  "version": "0.1.1",
  "private": true,
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "babel-jest": "^24.8.0",
    "fa-svelte": "^3.0.0",
    "jest": "^24.8.0",
    "jest-transform-svelte": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.10.1",
    "rollup-plugin-bundle-html": "^0.2.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p api:dev start:dev autobuild",
    "start": "sirv dist --single",
    "start:dev": "sirv dist --single --dev",
    "api:dev": "cd backend; go run ./cmd",
    "now-build": "yarn build",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "jest-transform-svelte"
    }
  }
}
